<extension>
    <modules>
    
        <!--
            Customization modules for adding i18n labels and importing client-side dependencies for
            the web previewer plugins
        -->
        
        <module>
            <id>Presence</id>
            <customizations>
                <customization>
                    <targetPackageRoot>org.alfresco.components</targetPackageRoot>
                    <sourcePackageRoot>presence.custom.components</sourcePackageRoot>
                </customization>
            </customizations>
            <auto-deploy>true</auto-deploy>
        </module>
        
        <module>
            <id>Presence Config</id>
            <customizations>
                <customization>
                    <targetPackageRoot>org.alfresco.components.dashlets</targetPackageRoot>
                    <sourcePackageRoot>presence.custom.components.dashlets.presence-config</sourcePackageRoot>
                </customization>
            </customizations>
            <auto-deploy>true</auto-deploy>
            <configurations>
            	
        
            </configurations>
        </module>
     
    </modules>
</extension>